<%
  const pageInfo = {
    url: "/"
    <!-- url: "/codingkadai/02/nakazono/" -->
  };
-%>
<!DOCTYPE html>
<html lang="ja">
<%- include('./include/_head', {meta:pageInfo}); -%>
<body class="p-birthday">
  <main class="main">
    
    <div class="p-birthday__inner">
      <div class="p-wikipedia">
        <h3 class="p-wikipedia__title">Wikipediaの記事検索</h3>
        <p class="p-wikipedia__sub-title">あなたの尊敬する偉人を検索せよ！</p>
        <input id="keyword" type="text" placeholder="例：インターネット" class="p-wikipedia__input">
        <button id="btn" class="p-wikipedia__button">検索</button>
        <div id="content"></div>
      </div>

      <h1 class="p-birthday__title">
        <span class="p-birthday__tag">必見<span class="_upright">!!</span></span>
        <span class="p-birthday__title-container">その辺にいる人の<br>誕生日一覧</span>
      </h1>

      <!-- STEP3 -->
      <section class="p-birthday__step">
        <h2 class="p-birthday__step-title">STEP3<span>ボタンをクリックして誕生日一覧を表示</span></h2>
        <a id="birthdayButton" class="p-birthday__button cubic">
          <span class="hovering">クリックしな！</span><span class="default">誕生日一覧を見る</span>
        </a>
        <ul id="birthday1" class="p-birthday__list"></ul>
      </section>

      <!-- STEP4 -->
      <section class="p-birthday__step">
        <h2 class="p-birthday__step-title">STEP4<span>ページを立ち上げた際に生成</span></h2>
        <ul id="birthday2" class="p-birthday__list"></ul>
      </section>
    </div>






  </main>
  <script defer src="<%= pageInfo.url %>assets/js/bundle.js"></script>
</body>
</html>
